{% extends 'baseadmin.html.twig' %}

{% block title %}Dashboard &mdash; Stisla

{% endblock %}

{% block stylesheets %}
{% endblock %}

{% block body %}

<div class="main-content">
    <section class="section">
      <h1 class="section-header">
        <div>Dashboard</div>
      </h1>
      <section class="section">
        <div class="container mt-5">
          <div class="row">
            <div class="col-12 col-sm-8 offset-sm-2 col-md-6 offset-md-3 col-lg-6 offset-lg-3 col-xl-4 offset-xl-4">
              
      
              <div class="card card-primary">
                <div class="card-header"><h4>Classe</h4></div>
      
                <div class="card-body">
                  <p class="text-muted">Veuillez choisir la classe pour 
                      laquelle vous souhaitez voir l'emploi du temps</p>
                  <form method="POST">
                      {{form_start(formulaire)}}
                    <div class="form-group">
                      <label for="email">Classe</label>
                     

                      {{form_widget(formulaire.name)}}
                      <!-- <input id="email" type="email" class="form-control" name="email" tabindex="1" required autofocus> -->
                    </div>
      
                    <div class="form-group">
                      <button type="submit" class="btn btn-primary btn-block" tabindex="4">
                        Consulter l'emploi du temps
                      </button>
                    </div>
                    {{form_end(formulaire)}}
                  </form>
                </div>
              </div>
              <div class="simple-footer">
                Copyright &copy; Stisla 2018
              </div>
            </div>
          </div>
        </div>
      </section>
    </section>
  </div>



{% endblock %}
{% block javascripts %}
      
{% endblock %}